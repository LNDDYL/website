{% extends "AdminBundle::backend.html.twig" %}

{% block content %}
    <h1>Article creation</h1>

    <form action="{{ path('admin_article_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">Create</button>
        </p>
    </form>

    <a class="awesome" href="{{ path('admin_article') }}">
        Back to the list
    </a>
{% endblock %}