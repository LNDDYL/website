{% extends "AndroBundle::frontend.html.twig" %}

{% block title %}Contact{% endblock %}

{% block content %}
    <p>{{ 'contact.desc'|trans }}</p>

    <form class="form-horizontal" action="{{ path('contact') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}

        <div class="form_submit">
            <input type="submit" value="{{ 'contact.submit'|trans }}"/>
        </div>
    </form>
{% endblock %}
