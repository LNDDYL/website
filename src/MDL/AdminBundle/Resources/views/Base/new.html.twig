{% extends "AdminBundle::backend.html.twig" %}

{% block page %}
    <h1>{{ block('title') }}</h1>

    <form action="{{ path('admin_' ~ entity_name ~ '_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit">Create</button>
        </p>
    </form>

    <a class="awesome" href="{{ path('admin_' ~ entity_name ~ '_list') }}">
        Back to the list
    </a>
{% endblock %}